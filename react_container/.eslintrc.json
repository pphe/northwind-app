{
    "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:jest/recommended"],
    "parser": "babel-eslint",
    "rules": {
        "no-console": "off",
        "indent": "off",
        "comma-dangle": ["error", { "objects": "only-multiline" }],
        "class-methods-use-this": 0
    },
    "plugins": ["jest"],
    "env": { "jest/globals": true }
}
